{
  "name": "webpack-noframe-template",
  "version": "1.0.0",
  "description": "简单页面使用webpack打包",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=mock node server.js",
    "deploy:mock": "rm -rf dist && NODE_ENV=mock webpack --config webpack.config.deploy.js --progress --colors -d -p",
    "deploy:prod": "rm -rf dist && NODE_ENV=production webpack --config webpack.config.deploy.js --progress --colors -p",
    "deploy:test": "rm -rf dist && NODE_ENV=test webpack --config webpack.config.deploy.js --progress --colors -d -p"
  },
  "repository": "git@github.com:sammffl/webpack-noframe-template.git",
  "author": "sammffl <samfec@163.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "color": "^1.0.3",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.5",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.25.0",
    "ip": "^1.1.4",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.14.0"
  },
  "dependencies": {
    "chart.js": "^2.5.0",
    "jquery": "^3.1.1",
    "zepto": "^1.2.0"
  }
}
